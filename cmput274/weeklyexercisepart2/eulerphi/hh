#include <iostream>

#include <iomanip>

using namespace std;

double 
int r[0] = a, r[1] = b ;
int s[0] = 1, s[1] = 0 ;
int t[0] = 0, t[1] = 1 ;


int i = 1;
while r[i] > 0 {
	int q = r[i-1]/r[i]
	r[i+1] = r[i-1] - q*r[i]
	s[i+1] = s[i-1] - q*s[i]
	t[i+1] = t[i-1] - q*t[i]
	i = i+1
}

uint32_t gcd_euclid_fast(uint32_t a, uint32_t b) {
  while (b > 0) {
    a %= b;

    // now swap them
    uint32_t tmp = a;
    a = b;
    b = tmp;
  }
  return a; // b is 0
}

